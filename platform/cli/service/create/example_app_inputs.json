{
    "provider": "aws",
    "accounts": {
        "sandbox": "launch-sandbox-admin"
    },
    "naming_prefix": "demo",
    "skeleton": {
        "url": "https://github.com/launchbynttdata/lcaf-template-java.git",
        "tag": "main"
    },
    "repos": {
        "application": {
            "repo": "",
            "tag": ""
        },
        "service": {
            "repo": "https://github.com/NTTDATA-Launch/launch-api-hex-java-template.git",
            "tag": "0.1.1"
        },
        "pipeline": {
            "repo": "https://github.com/nexient-llc/tf-aws-wrapper_module-codepipelines.git",
            "tag": "0.1.1"
        },
        "webhook": {
            "repo": "https://github.com/nexient-llc/tf-aws-wrapper_module-bulk_lambda_function.git",
            "tag": "0.1.0"
        }
    },
    "platform": {
        "configs" : {
            "application": {
                "templates": [
                    "common-application-config-non-secret.env",
                    "common-application-config-secret.env",
                    "common-application-config-secret-arns.env"
                ]
            }
        },
        "service": {
            "templates": {
                "application": {
                    "secret": "./platform/service/templates/application/secret.yaml",
                    "non_secret": "./platform/service/templates/application/non_secret.yaml"
                }
            },
            "sandbox": {
                "us-east-2": {
                    "000": {
                        "properties_file": "/exact/path/service/sandbox/us-east-2/000/terraform.tfvars",
                        "template_properties": {
                            "application": "./platform/service/sandbox/us-east-2/000/template_properties/application.yaml"
                        }
                    }
                }
            }
        },
        "pipeline": {
            "pipeline-provider": {
                "root": {
                    "us-east-2": {
                        "000": {
                            "properties_file": "/exact/path/pipeline/pipeline-provider/root/us-east-2/000/terraform.tfvars"
                        }
                    }
                }
            },
            "webhook-provider": {
                "root": {
                    "us-east-2": {
                        "000": {
                            "properties_file": "/exact/path/pipeline/webhook-provider/root/us-east-2/000/terraform.tfvars"
                        }
                    }
                }
            }
        }
    }
}